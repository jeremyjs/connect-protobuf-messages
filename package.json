  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "open-api-protobuf-messages-v59": "jeremyjs/Open-API-2.0-protobuf-messages#v5.9.1",
    "open-api-protobuf-messages-v60": "jeremyjs/Open-API-2.0-protobuf-messages#v6.0.0",
    "protobufjs": "4.1.3"
  },
  "devDependencies": {
    "@types/node": "11.12.1",
    "ava": "1.4.1",
    "nyc": "13.3.0",
    "ts-node": "8.0.3",
    "typescript": "3.4.1"
  }
}
